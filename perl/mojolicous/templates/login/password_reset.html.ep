#iTest{
widows:440px;
height:12px;
border:1 solid #000000;
}
.test{
	font-size:20px; 
	color:red;
	text-align:center;
	margin-top:0px;
	vertical-align: text-bottom;
	padding-top:18px;
}
</style>

